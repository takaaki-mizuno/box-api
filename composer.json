{
  "name": "takaaki-mizuno/box-sdk",
  "description": "Box.com PHP SDK Library ( Unofficial and not Comprehensive )",
  "keywords": [
    "box",
    "box.com",
    "sdk"
  ],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Takaaki Mizuno",
      "email": "takaaki.mizuno@gmail.com",
      "homepage": "http://takaaki.info/"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "firebase/php-jwt": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "friendsofphp/php-cs-fixer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "TakaakiMizuno\\Box\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/BaseClientTestCase.php"
    ]
  },
  "minimum-stability": "stable"
}
